{
  "manifest_version": 3,
  "name": "Storygraph Printer",
  "version": "1.1.0",
  "description": "Extracts 'To Read' information from Storygraph.",
  "action": {
    "default_title": "Storygraph Printer", 
    "default_popup": "index.html"
  },
  "icons": {
    "16": "sgp16.png",
    "32": "sgp32.png",
    "48": "sgp48.png",
    "128": "sgp128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": ["runPrint.js"],
      "matches": [
        "https://app.thestorygraph.com/*"
      ]
    }
  ]
}
