{
  "name": "storygraph_printer",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "watch": "nodemon --watch src --watch static --exec \"vite build\" -e json,ts,css,scss,html,svelte,js",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/chrome": "^0.0.258",
    "nodemon": "^3.0.3",
    "sass": "^1.69.5",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "sveltekit-adapter-chrome-extension": "^2.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module"
}
